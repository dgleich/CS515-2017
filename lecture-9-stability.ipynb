{"cells":[{"outputs":[],"cell_type":"code","source":["## Intro to stability\n","\n","function hilb(n::Integer)\n","  A = zeros(n,n)\n","  for j=1:n\n","    for i=1:n\n","      A[i,j] = 1.0/(i+j-1)\n","    end\n","  end\n","  return A\n","end\n","display(hilb(5))\n"],"metadata":{"autoscroll":false,"collapsed":false},"execution_count":null}],"nbformat_minor":0,"metadata":{"kernelspec":{"name":"julia-0.6","display_name":"Julia 0.6","language":"julia"}},"nbformat":4}